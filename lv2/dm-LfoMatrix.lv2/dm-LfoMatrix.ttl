@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix pprops:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .

<https://github.com/davemollen/dm-LfoMatrix>
	a lv2:Plugin , lv2:UtilityPlugin ;
	lv2:project <http://lv2plug.in/ns/lv2> ;
	doap:name "dm-LfoMatrix" ;
	doap:license <http://opensource.org/licenses/isc> ;
	doap:maintainer [
		foaf:name "Dave Mollen" ;
		foaf:homepage <https://github.com/davemollen/dm-LfoMatrix> ;
	] ;
	mod:brand "DM" ;
	mod:label "LfoMatrix" ;
	rdfs:comment """
A Control Voltage LFO plugin effect written in Rust.
""" ;
	lv2:requiredFeature lv2:inPlaceBroken ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "lfo1_on" ;
		lv2:name "On" ;
		lv2:portProperty lv2:integer, lv2:toggled;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "lfo1_freq" ;
		lv2:name "Freq" ;
		lv2:portProperty pprops:logarithmic ;
		lv2:default 2.0 ;
		lv2:minimum 0.1 ;
		lv2:maximum 30.0 ;
		units:unit units:hz
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "lfo1_shape" ;
		lv2:name "Shape" ;
		lv2:portProperty lv2:integer;
		lv2:portProperty lv2:enumeration ;
		lv2:default 1 ;
		lv2:minimum 1 ;
		lv2:maximum 8 ;
		lv2:scalePoint [rdfs:label "sine"; rdf:value 1];
		lv2:scalePoint [rdfs:label "triangle"; rdf:value 2];
		lv2:scalePoint [rdfs:label "saw up"; rdf:value 3];
		lv2:scalePoint [rdfs:label "saw down"; rdf:value 4];
		lv2:scalePoint [rdfs:label "square"; rdf:value 5];
		lv2:scalePoint [rdfs:label "sample and hold"; rdf:value 6];
		lv2:scalePoint [rdfs:label "random"; rdf:value 7];
		lv2:scalePoint [rdfs:label "curved random"; rdf:value 8];
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "lfo1_to_lfo1" ;
		lv2:name "Lfo1 to Lfo1" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "lfo1_to_lfo2" ;
		lv2:name "Lfo1 to Lfo2" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "lfo1_to_out1" ;
		lv2:name "Lfo1 to Out1" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "lfo1_to_out2" ;
		lv2:name "Lfo1 to Out2" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "lfo2_on" ;
		lv2:name "On" ;
		lv2:portProperty lv2:integer, lv2:toggled;
		lv2:default 1 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "lfo2_freq" ;
		lv2:name "Freq" ;
		lv2:portProperty pprops:logarithmic ;
		lv2:default 2.0 ;
		lv2:minimum 0.1 ;
		lv2:maximum 30.0 ;
		units:unit units:hz
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 9 ;
		lv2:symbol "lfo2_shape" ;
		lv2:name "Shape" ;
		lv2:portProperty lv2:integer;
		lv2:portProperty lv2:enumeration ;
		lv2:default 1 ;
		lv2:minimum 1 ;
		lv2:maximum 8 ;
		lv2:scalePoint [rdfs:label "sine"; rdf:value 1];
		lv2:scalePoint [rdfs:label "triangle"; rdf:value 2];
		lv2:scalePoint [rdfs:label "saw up"; rdf:value 3];
		lv2:scalePoint [rdfs:label "saw down"; rdf:value 4];
		lv2:scalePoint [rdfs:label "square"; rdf:value 5];
		lv2:scalePoint [rdfs:label "sample and hold"; rdf:value 6];
		lv2:scalePoint [rdfs:label "random"; rdf:value 7];
		lv2:scalePoint [rdfs:label "curved random"; rdf:value 8];
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 10 ;
		lv2:symbol "lfo2_to_lfo1" ;
		lv2:name "Lfo2 to Lfo1" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 11 ;
		lv2:symbol "lfo2_to_lfo2" ;
		lv2:name "Lfo2 to Lfo2" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 12 ;
		lv2:symbol "lfo2_to_out1" ;
		lv2:name "Lfo2 to Out1" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 13 ;
		lv2:symbol "lfo2_to_out2" ;
		lv2:name "Lfo2 to Out2" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	], [
		a lv2:CVPort, lv2:OutputPort ;
		lv2:index 14 ;
		lv2:symbol "out1" ;
		lv2:name "Out1"
	] , [
		a lv2:CVPort, lv2:OutputPort ;
		lv2:index 15 ;
		lv2:symbol "out2" ;
		lv2:name "Out2"
	] .